= render 'time_form' do |f|
  = f.input :risk_level_eq, collection: (1..3), include_blank: 'any', input_html: { style: 'max-width: 100px' }    
  = f.input :address_eq, as: :string, input_html: { style: 'max-width: 500px' }
  = f.input :cc_code_eq, collection: AddressAnalysis.group(:cc_code).count.keys, input_html: { style: 'max-width: 100px' }, include_blank: 'any'
